{
    "name": "playerom/laracsv",
    "type": "library",
    "description": "A Laravel package to easily generate CSV files from Eloquent model.",
    "homepage": "https://github.com/usmanhalalit/laracsv",
    "keywords": ["laravel", "csv", "eloquent", "export"],
    "license": "MIT",
    "authors": [
        {
            "name": "Muhammad Usman",
            "email": "hi@usman.it",
            "role": "Developer"
        }
    ],

    "require": {
        "php": "^8.3",
        "illuminate/database": "^11.0",
        "league/csv": "^9.21"
    },

    "require-dev": {
        "fakerphp/faker": "^1.24",
        "phpunit/phpunit": "^12.0"
    },

    "autoload":	{
        "psr-4": {
            "Playerom\\Laracsv\\": "src/"
        }
    },

    "autoload-dev": {
        "psr-4": {
            "Playerom\\Laracsv\\Tests\\": "tests/"
        },

        "classmap": [
            "tests/TestCase.php"
        ]
    }
}
